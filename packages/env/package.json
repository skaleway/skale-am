{
  "name": "@skaleam/env",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "exports": {
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js"
    }
  },
  "scripts": {
    "build": "tsdown client.ts server.ts --format esm,cjs --dts -d dist && cp dist/client-*.d.ts dist/client.d.ts 2>/dev/null || true && cp dist/server-*.d.ts dist/server.d.ts 2>/dev/null || true"
  },
  "devDependencies": {
    "@skaleam/eslint-config": "workspace:*",
    "@skaleam/typescript-config": "workspace:*",
    "@types/node": "^20"
  },
  "dependencies": {
    "@t3-oss/env-core": "^0.13.8",
    "dotenv": "^16.5.0",
    "tsdown": "^0.15.6",
    "zod": "^4.0.14"
  }
}
